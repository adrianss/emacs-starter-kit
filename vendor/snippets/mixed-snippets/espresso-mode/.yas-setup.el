(set 'js-ajax-datatype '("xml" "html" "script" "json" "jsonp" "text"))

(set 'http-content-type '("application/x-www-form-urlencoded" "maybeother?"))

(set 'js-jquery-support '("boxModel" "cssFloat" "hrefNormalized" "htmlSerialize"
                          "leadingWhitespace" "noCloneEvent" "objectAll"
                          "opacity" "scriptEval" "style" "tbody"))
