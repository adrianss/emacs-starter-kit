;; options

;; Events
(set 'js-events '("blur" "focus" "load" "resize"
 "scroll" "unload" "click" "dblclick" "mousedown" "mouseup"
 "mousemove" "mouseover" "mouseout" "mouseenter" "mouseleave"
 "change" "select" "submit" "keydown" "keypress" "keyup"
 "error" "custom"))

